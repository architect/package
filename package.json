{
  "name": "@architect/package",
  "version": "11.0.0-RC.0",
  "description": "Package .arc for deployment with CloudFormation",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "test": "npm run lint && npm run coverage",
    "test:nolint": "npm run coverage",
    "test:unit": "cross-env tape 'test/unit/**/*-test.js'",
    "test:integration": "cross-env tape 'test/integration/**/*-test.js'",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/architect/package.git"
  },
  "author": "Brian LeRoux <b@brian.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/inventory": "~6.0.0-RC.0",
    "@architect/utils": "~6.0.0-RC.1"
  },
  "devDependencies": {
    "@architect/eslint-config": "~3.0.0",
    "@aws-lite/client": "~0.23.2",
    "@aws-lite/s3": "~0.2.6",
    "cross-env": "~10.0.0",
    "eslint": "~9.36.0",
    "mock-tmp": "~0.0.4",
    "nyc": "~17.1.0",
    "tape": "~5.9.0"
  }
}
