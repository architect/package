{
  "name": "@architect/package",
  "version": "6.2.2",
  "description": "Package .arc for deployment with CloudFormation",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "test:unit": "cross-env PORT=6666 tape 'test/unit/**/*-test.js' | tap-spec",
    "test:slow": "cross-env PORT=6666 tape 'test/slow/**/*-test.js' | tap-spec",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "test": "npm run lint && npm run coverage",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/architect/package.git"
  },
  "author": "Brian LeRoux <b@brian.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/inventory": "~2.0.2",
    "@architect/utils": "~3.0.1",
    "chalk": "~4.1.1"
  },
  "devDependencies": {
    "@architect/eslint-config": "~1.0.0",
    "aws-sdk": "~2.952.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "mock-fs": "~5.0.0",
    "nyc": "^15.1.0",
    "run-parallel": "~1.2.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.2.2"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config"
  }
}
